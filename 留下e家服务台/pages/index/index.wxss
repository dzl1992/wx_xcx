/**index.wxss**/
page {
  position: relative;
}
.header {
  width: 100%;
  height: 444rpx;
}

.header image {
  width: 100%;
  height: 100%;
}

.swiper {
  width: 95%;
  line-height: 96rpx;
  background-color: #f1f1f1;
  padding-left: 5%;
}
.swiper-tab {
  display: inline-block;
  width: 220rpx;
  font-size: 28rpx;
  text-align: center;
  color: #777;
}
.swiper-tab-selected0 {
  background: white;
  clip-path: polygon(0 0, 200rpx 0, 220rpx 96rpx, 0 96rpx);
}
.swiper-tab-selected1 {
  background: white;
  clip-path: polygon(20rpx 0, 200rpx 0, 220rpx 96rpx, 0 96rpx);
}

.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
}
.swiper-box-item {
  margin: auto;
}

.aprtment-box {
  margin: 48rpx 35rpx;
}
.aprtment-box-title {
  font: 32rpx bold;
}
.aprtment-box-container {
  width:100%;
  display:flex;
  flex-direction:row;
  flex-wrap:wrap;
  justify-content:flex-start;
}

.aprtment_items {
  width: 300rpx;
  margin-top: 10rpx;
  margin-right: 18rpx;
  padding: 16rpx;
  display: flex;
  flex-direction:column;
  align-items: center;
  background: #f1f1f1;
  border-radius: 5px;
}
.aprtment_items:nth-child(2n) {
  margin-right: 0;
}
.aprtment_items image {
  width: 300rpx;
  height: 200rpx;
}
.aprtment_items view {
  text-align:left;
  font-size: 26rpx;
  margin-top:10rpx;
  /* display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1; */
  overflow:hidden;
  width:100%;
}
.aprtment_items text {
  font-size: 26rpx;
  width:100%;
  text-align: left;
  margin-top:10rpx;
}
.price {
  color: red;
}
.priceTagLine {
  color:#aaa;
  margin-left:10px;
  text-decoration:line-through;
}

.split-line {
  width: 100%;
  height: 16rpx;
  background: #f7f6fc;
}

.menu-box-container {
  margin: auto 35rpx;
}

.menu-items {
  display:flex;
  flex-direction:row;
  align-items:center;
  justify-content:space-between;
  border-bottom:1px solid #f1f1f1;
  height:128rpx;
}
.menu-items > view {
  display:flex;
  flex-direction:row;
  align-items:center;
}
.menu-items > image {
  width: 64rpx;
  height: 64rpx;
  margin-right: -10rpx;
}
.menu-items-icon {
  width: 90rpx;
  height: 90rpx;
  border-radius: 4rpx;
}
.menu-items-info {
  display:flex;
  flex-direction:column;
  justify-content:space-between;
  align-items:flex-start;
  margin-left: 10rpx;
  position: relative;
}
.menu-items-info > view {
  font-size: 28rpx;
  font-weight: bold;
  color: #333333;
}
.menu-items-info > text {
  margin-top: 16rpx;
  font-size: 24rpx;
  color: #999999;
}
.model {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  position: absolute;
  top: 0;
  left: 0;
}
.model_phone {
  background: rgba(0, 0, 0, 0.3);
}
.model_cen {
  width: 80%;
  height: 360rpx;
  background-color: #fff;
  margin: 200rpx auto;
  padding: 5% 5%;
  position: relative;
  font-size: 28rpx;
}
.APP_cen {
  position: absolute;
  bottom: 220rpx;
  left: 5%;
}
.ScM {
  position: absolute;
  top: 0rpx;
  left: 5%;
}
.model_cen > view {
  font-size: 30rpx;
  width: 100%;
  margin-top: 20rpx;
  text-align: left;
  line-height: 50rpx;
}
.model_cen > button {
  margin-top: 50rpx;
  display: inline-block;
  background-color: #3399ff;
  color: #fff;
  width: 140rpx;
  height: 60rpx;
  line-height: 60rpx;
  position: absolute;
  right: 32rpx;
  border-radius: 4rpx;
  font-size: 28rpx;
}
.model_cen > button::after {
  border: none;
}
#ksbz {
  font-size:28rpx;
  font-weight:bold;
  color:#333333;
  background-color: #fff;
  width: auto;
  padding: 0;
  margin: 0;
  border-left: none;
  border-right: none;
  border-top: none;
}
#ksbz:after { 
  border: none;
}
.tex {
  position: absolute;
  font-weight: normal;
  bottom: 20rpx;
}
.model_cen > view > text {
  color: green;
}
.model_cen > text {
  margin-top: 50rpx;
  display: inline-block;
  background-color: #fff;
  color: #333;
  width: 70rpx;
  height: 60rpx;
  line-height: 60rpx;
  position: absolute;
  right: 240rpx;
  border-radius: 4rpx;
}
.titImg {
  width: 100%;
  height: 90rpx;
  text-align: center;
}
.titImg > image {
  display: inline-block;
  width: 80rpx;
  height: 80rpx;
  margin-left: 42%;
}
.kksqF {
  position: relative;
}
.kksq {
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: transparent;
}
.kksq::after {
  border: none;
}
.pf {
  width: 100rpx;
  height: 110rpx;
  text-align: center;
  padding: 5rpx 0;
  position: absolute;
  right: 50rpx;
  bottom: 420rpx;
  z-index: 100;
  background-color: rgba(0, 0, 0, 0.2);
  border-radius: 50%;
}
.pf::after {
  border: none;
}
.pf > image {
  width: 96rpx;
  height: 96rpx;
}