/* pages/zssb/zssb.wxss */
page {
  width: 100%;
  height: 100%;
}
.content {
  width: 100%;
  height: 100%;
  background-color: #f7f6fc;
}
.content0 {
  width: 100%;
  height: 100%;
}
.No_cen {
  width: 50%;
  height: 25%;
  position: absolute;
  left: 25%;
  top: 20%;
  text-align: center;
}
.No_cen > image {
  width: 100rpx;
  height: 100rpx;
} 
.No_cen > view {
  font-size: 28rpx;
  color: #999;
}
.content1 {
  width: 100%;
  height: 100%;
  padding-top: 32rpx;
}
.cen_index {
  width: 100%;
  height: 130rpx;
  background-color: #fff;
  border-top: 1rpx solid #e0e0e0;
  position: relative;
}
.cen_index:last-child {
  border-bottom: 1rpx solid #e0e0e0;
  margin-bottom: 200rpx;
}
.left {
  width: 14%;
  height: 100%;
  display: inline-block;
  position: absolute;
}
.left > image {
  width: 80rpx;
  height: 80rpx;
  margin: 26rpx 20.44%;
}
.right {
  width: 84%;
  height: 100%;
  display: inline-block;
  position: absolute;
  right: 0;
}
.right > .house {
  position: absolute;
  top: 32rpx;
  font-size: 28rpx;
  color: #333;
  width: 400rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.right > text {
  position: absolute;
  top: 80rpx;
  font-size: 28rpx;
  color: #999;
}
.phone {
  left: 120rpx;
}
.time {
  display: inline;
  position: absolute;
  right: 40rpx;
  top: 32rpx;
  font-size: 28rpx;
  color: #333;
}
.resion {
  display: inline-block;
  position: absolute;
  right: 40rpx;
  top: 80rpx;
  font-size: 24rpx;
  color: #ccc;
  width: 150rpx;
  height: 30rpx;
  overflow:hidden;
  text-overflow:ellipsis;
}
.btn {
  display: block;
  width: 100%;
  position: fixed;
  bottom: 0;
  border-radius: 4rpx;
}
.name {
  display: inline-block;
  width: 140rpx;
  height: auto;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}