/* pages/wode/wode.wxss */
page {
  width: 100%;
  height: 100%;
}
.entry {
  width: 100%;
  height: 100%;
}
.top {
  width: 100%;
  height: 200rpx;
  position: relative;
}
.beijing {
  width: 100%;
  height: 100%;
}
.touxiang {
  width: 140rpx;
  height: 140rpx;
  position: absolute;
  top: 30rpx;
  left: 30rpx;
  z-index: 999;
  border-radius: 50%;
}
.dlzc {
  width: 500rpx;
  position: absolute;
  top: 70rpx;
  left: 200rpx;
  z-index: 999;
  font-size: 28rpx;
  color: #3399ff;
}
.dlzc > button {
  display: inline-block;
  background-color: transparent;
  font-size: 28rpx;

}
button::after {
   border: none; 
}
.center {
  width: 91.11%;
  margin: 16rpx auto;

}
.wdsc > image {
  width: 32rpx;
  height: 32rpx;
  margin-bottom: -6rpx;
}
.wdsc > view {
  display: inline-block;
  font-size: 28rpx;
  margin-left: 32rpx;
}
.kf {
  margin-top: 20rpx;
}
.kf > image {
  width: 32rpx;
  height: 32rpx;
  margin-bottom: -6rpx;
}
.kf > view {
  display: inline-block;
  font-size: 28rpx;
  margin-left: 32rpx;
}
.kf > text {
  float: right;
  color: #3399ff;
  font-size: 28rpx;
}